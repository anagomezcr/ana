function find(source, find)
{
  pos = -1;

  for(i = 0; i < source.length(); i++)
  {
    if(source[i] == find[0])
    {
      x = 0;
      pos = i;
      matched = true;

      for(j = i; j < source.length(); j++)
      {
        if(x < find.length()) 
        {
          if(source[j] != find[x++])
          {
            matched = false;
            break;
          }
        }
      }

      if(matched)
      {
        break;
      }         
    }       
  }

  return pos;
}


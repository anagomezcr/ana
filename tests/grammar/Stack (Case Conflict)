class Stack {  
  public function stack()
  {
    self.items = [];
    self.pos = [];
  }
  public function push(value)
  {
    self.items[self.pos++] = value;
  }

  public function pop()
  {
    return self.items[--self.pos];
  }
}

function main()
{
  stack = Stack();


  stack.push(1);

}



main();

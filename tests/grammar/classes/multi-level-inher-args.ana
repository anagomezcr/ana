class Grandparent {
  public function Grandparent(arg1)
  {
    print("Constructing Grandparent");
  }

  public function getGrandParentName() 
  {
    return "Grandparent";
  }
}

class Parent : Grandparent {
  public function Parent(arg1)
  {
    print("Constructing Parent base is (" + base + ")");
  }
  public function getParentName() 
  { 
    return "Parent"; 
  }
}

class Child : Parent {
  public function Child()
  {
    print("Constructing child base is (" + base + ")");
  } 
  public function getChildName() 
  { 
    return "" + self; 
  }
}

class Wrapped {

  public function Wrapped(adapter)
  {
    self.adapter = adapter;
  }
}

wrapper = Wrapped(Child);

child = wrapper.adapter();



print(child);
